<% engine.each_process do |name, process| %>
  <% 1.upto(engine.formation[name]) do |num| %>
God.watch do |w|
  w.name = "<%= "#{name}-#{num}" %>"
  w.start = "export PORT=<%= engine.port_for(process, num) %>;<%= process.command %>"
  w.keepalive
  w.group = "<%= app  %>"
  w.dir = "<%= engine.root %>"
    <%= extension(name) %>
end
  <% end %>
<% end %>
